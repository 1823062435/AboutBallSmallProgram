<!--pages/bollplace/bollplace.wxml-->
<view class="bigbox" wx:for="{{list}}">
  <image src="{{item.img}}" class="img"></image>
  <view class="box">
    <view class="name">{{item.name}}</view>
    <view class="placename">{{item.placename}}</view>
    <view class="kind1">{{item.kind1}}</view>
    <view class="content">{{item.content}}</view>
  </view>
</view>
